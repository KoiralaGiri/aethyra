{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./*"]
      }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  